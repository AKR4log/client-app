{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"somi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/components/(.*)$",
		"^@/ui/(.*)$",
		"^@/assets/(.*)$",
		"^@/config/(.*)$",
		"^@/hook/(.*)$",
		"^@/pages/(.*)$",
		"^@/services/(.*)$",
		"^@/store/(.*)$",
		"^@/types/(.*)$",
		"^@/utils/(.*)$",
		"^../(.*)$",
		"^./(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
